{
  "name": "project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" FORCE_COLOR=1 UV_THREADPOOL_SIZE=1 JEST_WORKER_ID=1 next dev",
    "dev:safe": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096 --no-experimental-fetch\" UV_THREADPOOL_SIZE=1 JEST_WORKER_ID=1 next dev",
    "build": "next build",
    "start": "next start -p 3001",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "cypress": "cypress open",
    "cypress:headless": "cypress run",
    "cypress:ci": "start-server-and-test dev http://localhost:3000 cypress:headless",
    "test:e2e": "start-server-and-test dev http://localhost:3000 cypress",
    "test:a11y": "playwright test --config=playwright.config.ts",
    "test:a11y:ui": "playwright test --config=playwright.config.ts --ui",
    "test:all": "npm run test:ci && npm run test:a11y && npm run cypress:ci",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@sentry/nextjs": "^10.10.0",
    "@sentry/tracing": "^7.120.4",
    "@supabase/supabase-js": "^2.54.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "lucide-react": "^0.537.0",
    "next": "15.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "slugify": "^1.6.6",
    "tailwind-merge": "^3.3.1",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "autoprefixer": "^10.4.21",
    "axe-core": "^4.10.3",
    "cross-env": "^10.0.0",
    "cypress": "^15.1.0",
    "cypress-axe": "^1.7.0",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "eslint-plugin-cypress": "^5.1.1",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-testing-library": "^7.6.8",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "playwright": "^1.55.0",
    "postcss": "^8.5.6",
    "puppeteer": "^24.16.2",
    "start-server-and-test": "^2.1.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5"
  }
}
